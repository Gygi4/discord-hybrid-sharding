    "devDependencies": {
        "@types/node": "^18.11.18",
        "@types/node-fetch": "^2.6.2",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "clean-jsdoc-theme": "^4.2.2",
        "discord.js": "^14.7.1",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-jsdoc": "^39.6.6",
        "eslint-plugin-prettier": "^4.2.1",
        "jsdoc": "^4.0.0",
        "prettier": "^2.8.3",
        "typescript": "^4.9.4"
